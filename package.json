{
  "name": "with-retries",
  "version": "0.1.0",
  "description": "with-retries is a utility for retrying promises upon failure",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "build": "grunt",
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern node_modules . --ext .js,.ts --cache",
    "lint:fix": "eslint --ignore-path .gitignore --ignore-pattern node_modules  . --ext .js,.ts --fix --cache",
    "test": "jest",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "prepush": "yarn lint && yarn test",
    "prepublishOnly": "yarn prepush && yarn build",
    "publish-package": "yarn publish dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattm-malone/with-retries.git"
  },
  "keywords": [
    "with",
    "retry",
    "function",
    "promise",
    "await",
    "retries",
    "async"
  ],
  "author": "Matthew Malone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattm-malone/with-retries/issues"
  },
  "homepage": "https://github.com/mattm-malone/with-retries#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "auto-changelog": "^2.2.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "grunt": "^1.4.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-ts": "^6.0.0-beta.22",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-junit": "^12.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "D": "^1.0.0",
    "ts-node": "^9.1.1"
  }
}
